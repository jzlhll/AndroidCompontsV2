<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- app 主题背景色 -->
    <color name="windowBackground">#ffffff</color>
    <!-- app dialog的背景色 -->
    <color name="windowDialogBackground">#ffffff</color>

    <!-- 背景色 ：文字图标，或者一片框框-->
    <color name="color_normal_block0">#ffffff</color>
    <color name="color_normal_block">#f0f0f0</color>

    <!-- 品牌主题色 | 超链接文字颜色 -->
    <color name="colorPrimary">#248067</color>
    <color name="colorPrimary_disable">#85B79E</color>
    <color name="colorPrimary_press">#5f9483</color>

    <!-- 文字颜色 -->
    <color name="color_text_normal">#111111</color>
    <color name="color_text_normal_press">#dd222222</color>
    <string name="color_text_normal_str" translatable="false">"#111111"</string>
    <color name="color_text_desc">#888888</color>
    <string name="color_text_desc_str" translatable="false">"#888888"</string>

    <color name="color_text_reverse">#ffffff</color>
    <color name="color_text_reverse_press">#ccffffff</color>

    <color name="color_hint_text">#cccccc</color>

    <!-- button: main button色 -->
    <color name="color_main_btn_bg">#db4f4f</color>
    <color name="color_main_btn_bg_press">#d14b4b</color>
    <color name="color_main_btn_text">#ffffff</color>
    <color name="color_main_btn_text_press">#ccffffff</color>

    <!-- button: secondary button色 -->
    <color name="color_second_btn_bg">#f0f0f0</color>
    <color name="color_second_btn_bg_press">#f0f0f0</color>
    <color name="color_second_btn_text">@color/color_text_normal</color>
    <color name="color_second_btn_text_press">@color/color_text_normal_press</color>

    <!-- button: disable -->
    <color name="color_disable_btn_bg">#f6f6f6</color>
    <color name="color_disable_btn_text">@color/color_hint_text</color>

    <!-- button: warning button色 -->
    <color name="color_warn_btn_bg">#fa3939</color>
    <color name="color_warn_btn_bg_press">#ccfa3939</color>
    <color name="color_warn_btn_text">#ffffff</color>

    <!-- edit: 相关一切颜色 -->
    <color name="color_edit_text_hint">@color/color_hint_text</color>

    <color name="color_edit_background">#f0f0f0</color>
    <color name="color_edit_background_error">#66fa3939</color>

    <color name="color_edit_board_select">@color/color_text_normal</color>
    <color name="color_edit_board_normal">#cccccc</color>
    <color name="color_edit_board_error">#fa3939</color>

    <!-- tabLayout: 相关一切颜色。由于tabLayout有不同的样式。这里只提供一个常规的。 -->
    <color name="color_tab_text_no_select">@color/color_text_desc</color>
    <color name="color_tab_text_select">@color/color_text_normal</color>

    <!-- 分割线的颜色 -->
    <color name="color_line">#eeeeef</color>
    <color name="color_line_on_dialog">#eeeeef</color>
    <color name="color_line_on_block">#cccccc</color>

    <!-- switch控件的颜色 -->
    <color name="color_switch_block_bg">#f0f0f0</color>
    <color name="color_switch_block_sel_dis_bg">#e8e8e8</color>
    <color name="color_switch_block_text">@color/color_text_normal</color>
    <color name="color_switch_block_text_dis">@color/color_hint_text</color>
    <color name="color_switch_block_text_sel">@color/color_text_normal</color>
    <color name="color_switch_block_text_sel_dis">@color/color_text_desc</color>

    <!-- toolBar相关 -->
    <color name="color_toolbar_text">@color/color_text_normal</color>
    <color name="color_toolbar_background">#ffffff</color>

    <!-- glide 的基础颜色 -->
    <color name="color_glide_gray_default">#f0f0f0</color>

    <!-- 悬挂提醒DING信息背景色 -->
    <color name="color_ding_bg">#fff2e0</color>

    <!-- 悬浮floatingButton的背景色 -->
    <color name="color_floating_btn_bg">#8b414141</color>
</resources>